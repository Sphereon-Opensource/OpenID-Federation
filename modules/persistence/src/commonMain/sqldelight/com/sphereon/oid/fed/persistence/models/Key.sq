create:
INSERT INTO Jwk (
    account_id,
    kid,
    key
) VALUES (?, ?, ?)
RETURNING *;

revoke:
UPDATE Jwk
SET revoked_at = CURRENT_TIMESTAMP,
    revoked_reason = ?
WHERE id = ?
AND revoked_at IS NULL
RETURNING *;

findByAccountId:
SELECT *
FROM Jwk
WHERE account_id = ?
AND revoked_at IS NULL
ORDER BY created_at DESC;

findById:
SELECT *
FROM Jwk
WHERE id = ?;
